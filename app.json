{
  "expo": {
    "name": "Must-Watch",
    "slug": "must-watch",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mustwatchlogo.png",
    "splash": {
      "image": "./assets/images/mwsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A simple app for creating a visual list of movies/shows you want to watch. Search a movie then add it to your list.",
    "githubUrl": "https://github.com/marlbones/must-watch"
  }
}